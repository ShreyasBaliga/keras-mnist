"import mxnet as mx
",
"
",
"def to4d(img):
",
"    return img.reshape(img.shape[0], 1, 28, 28).astype(np.float32)/255
",
"
",
"batch_size = 100
",
"train_iter = mx.io.NDArrayIter(to4d(train_img), train_lbl, batch_size, shuffle=True)
",
"val_iter = mx.io.NDArrayIter(to4d(val_img), val_lbl, batch_size)"
]
